Pyton passos 
//Comandos Python
python -m venv venv   //Criar ambiente virtual
.\venv\Scripts\activate  //Start ambiente virtual 
pip install django // instalar django
python .\manage.py runserver  //Startr server django
python .\manage.py migrate // importar componentes.
python .\manage.py showmigrations // verificar status mymigrate
python .\manage.py  startapp blog // criar app 
python .\manage.py makemigrations // atualizar banco de dados
python .\manage.py sqlmigrate blog 0001 // atualizar banco de dados
python .\manage.py migrate blog // atualizar banco de dados
python .\manage.py migrate --run-syncdb
python .\manage.py  createsuperuser 
pip install ipython //ferramenta de sql
python .\manage.py shell abrir sql
pip install django-environ
pip install whitenoise

python .\manage.py sqlmigrate blog 0012_auto_20220307_1118


